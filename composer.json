{
  "name": "abethropalle/isonsei_no_cnunyu",
  "description": "Simple DI container.",
  "version": "0.5.0",
  "type": "library",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://git@github.com/Qurum/abethropalle-utils.git"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-simplexml": "*",
    "symfony/yaml": "*",
    "abethropalle/utils": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },

  "minimum-stability": "dev",

  "autoload": {
    "psr-4": {
      "Abethropalle\\IsonseiNoChunyu\\": "src\\",
      "Abethropalle\\IsonseiNoChunyu\\Tests\\": "tests\\src",
      "Abethropalle\\DataTransferProjectStub\\": "tests\\resources\\DataTransferProjectStub"
    }
  }
}